<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" layoutX="250.0" layoutY="45.0" prefHeight="50.0" prefWidth="500.0" text="Tournament Results">
         <font>
            <Font name="Noto Sans CJK JP Medium" size="25.0" />
         </font>
      </Label>
      <Button fx:id="rankingButton" layoutX="650.0" layoutY="375.0" mnemonicParsing="false" onAction="#rankingButtonAction" prefHeight="100.0" prefWidth="200.0" text="Ranking" />
      <Button fx:id="returnButton" layoutX="650.0" layoutY="579.0" mnemonicParsing="false" onAction="#returnButtonAction" prefHeight="100.0" prefWidth="200.0" text="Return" />
      <ScrollPane fx:id="matchPane" layoutX="118.0" layoutY="278.0" prefHeight="351.0" prefWidth="366.0" />
      <Label layoutX="274.0" layoutY="213.0" text="Matchs">
         <font>
            <Font name="Noto Sans CJK JP Medium" size="18.0" />
         </font>
      </Label>
      <Label layoutX="615.0" layoutY="213.0" text="Winner :">
         <font>
            <Font name="Noto Sans CJK JP Medium" size="18.0" />
         </font>
      </Label>
      <Label fx:id="winnerLabel" layoutX="724.0" layoutY="217.0" text="WinnerLabel">
         <font>
            <Font size="18.0" />
         </font></Label>
      <ScrollPane fx:id="roundPane" layoutX="205.0" layoutY="127.0" prefHeight="75.0" prefWidth="200.0" />
      <Button fx:id="sexButton" layoutX="775.0" layoutY="45.0" mnemonicParsing="false" onAction="#changeSex" prefHeight="50.0" prefWidth="150.0" text="Change Sex" />
      <Label fx:id="sexLabel" layoutX="775.0" layoutY="111.0" text="Men" />
   </children>
</AnchorPane>
