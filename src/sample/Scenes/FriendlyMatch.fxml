<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" layoutX="250.0" layoutY="72.0" prefHeight="50.0" prefWidth="500.0" text="Friendly Match">
         <font>
            <Font name="Noto Sans CJK JP Medium" size="25.0" />
         </font>
      </Label>
      <Label fx:id="player1Name" alignment="CENTER" layoutX="204.0" layoutY="170.0" text="Player 1">
         <font>
            <Font name="Noto Sans CJK JP Medium" size="24.0" />
         </font>
      </Label>
      <Label fx:id="player2Name" alignment="CENTER" layoutX="704.0" layoutY="170.0" text="Player 2">
         <font>
            <Font name="Noto Sans CJK JP Medium" size="24.0" />
         </font>
      </Label>
      <ScrollPane fx:id="player1Field" layoutX="111.0" layoutY="230.0" prefHeight="185.0" prefWidth="278.0" />
      <ScrollPane fx:id="player2Field" layoutX="600.0" layoutY="230.0" prefHeight="185.0" prefWidth="278.0" />
      <Button fx:id="randomButton" layoutX="151.0" layoutY="455.0" mnemonicParsing="false" onAction="#randomButtonAction" prefHeight="75.0" prefWidth="200.0" text="Random" />
      <Button fx:id="returnButton" layoutX="650.0" layoutY="455.0" mnemonicParsing="false" onAction="#returnButtonAction" prefHeight="75.0" prefWidth="200.0" text="Return" />
      <Button fx:id="launchButton" layoutX="375.0" layoutY="587.0" mnemonicParsing="false" onAction="#launchButtonAction" prefHeight="100.0" prefWidth="250.0" text="Launch" />
   </children>
</AnchorPane>
